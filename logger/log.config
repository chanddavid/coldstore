[loggers]
keys=root

[logger_root]
level=DEBUG
handlers=file_handler

[formatters]
keys=formatter

[handlers]
keys=file_handler

[handler_file_handler]
class=logging.handlers.TimedRotatingFileHandler
level=INFO
formatter=formatter
args=('sdk.log', 'W5', 0, 4)

[formatter_formatter]
format={"date":"%(asctime)s","level":"%(levelname)s","file":"%(filename)s","lineno":"%(lineno)d", "msg": "%(message)s"}
datefmt=%Y-%m-%d %H:%M:%S